{
	"manifest_version": 3,
	"name": "Questions Extension",
	"description": "Ask questions to ChatGPT.",
	"version": "1.0",
	"icons": {
		"32": "images/icon-32.png",
		"64": "images/icon-64.png",
		"128": "images/icon-128.png"
	},
	"background": {
		"service_worker": "fetch.js",
		"type": "module"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"32": "images/icon-32.png",
			"64": "images/icon-64.png",
			"128": "images/icon-128.png"
		}
	}
}